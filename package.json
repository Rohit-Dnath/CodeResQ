{
	"name": "DuckKode",
	"displayName": "DuckKode",
	"description": "Protecting your code from vulnerabilities and bugs.",
	"version": "0.0.2",
	"publisher": "Nalayak",
	"engines": {
		"vscode": "^1.70.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:crq.crq"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "crq.crq",
				"title": "DuckKode"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.10",
		"@types/node": "^22.13.14",
		"@types/vscode": "^1.70.0",
		"typescript": "^4.8.4",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"axios": "^1.8.4",
		"vsce": "^2.15.0"
	}
}
